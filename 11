<script src="https://cdn.jsdelivr.net/npm/qrcode_js@1.0.0/qrcode.min.js"></script>
<script>
(function(){
    var end_ts=1633071600;
    var prev_days=0;
    var prev_hours=0;
    var prev_minutes=0;
    var prev_seconds=0;
    var do_countdown=function(){
        var now_ts=Math.floor(new Date().getTime()/1000);
        var ts_diff=end_ts-now_ts;
        if(ts_diff<0){
            clearInterval(interval);
            location.reload();
            return;
        }
        var days=Math.floor(ts_diff/60/60/24);
        var hours=Math.floor((ts_diff-days*60*60*24)/60/60);
        var minutes=Math.floor((ts_diff-days*60*60*24-hours*60*60)/60);
        var seconds=Math.floor(ts_diff-days*60*60*24-hours*60*60-minutes*60);
        if(prev_days!=days){
            $('.count_down_days .number').text(days);
        }
        if(prev_hours!=hours){
            $('.count_down_hours .number').text(hours);
        }
        if(prev_minutes!=minutes){
            $('.count_down_minutes .number').text(minutes);
        }
        $('.count_down_seconds .number').text(seconds);
        prev_days=days;
        prev_hours=hours;
        prev_minutes=minutes;
        prev_seconds=seconds;
    };
    var interval=setInterval(do_countdown,1000);
    do_countdown();
})();
</script>
<script>
(function(){
    new ClipboardJS('.copy',{
        text:function(trigger) {
            var target=$(trigger);
            if(target.attr('target')){
                target=$(target.attr('target'));
            }
            return target.attr('text')||target.val()||target.text();
        }
    }).on('success',function(event){
        var trigger=$(event.trigger);
        var title_origin=trigger.attr('title');
        var message=trigger.attr('data-copy-success');
        trigger.removeAttr('title');
        trigger.tooltip({
            title:message?message:"Â§çÂà∂ÊàêÂäü",
            trigger:'manual',
        }).tooltip('show');
        setTimeout(function(){
            trigger.tooltip('destroy');
            trigger.attr('title',title_origin);
        },600);
    }).on('error',function(e){
        alert("Â§çÂà∂ÈîôËØØ");
    });
    window.init_page=function(selector){
        $(selector).find('.qrcode').popover({
            'content':'<div class="qrcode-title text-center" style="height:20px;"></div><div class="qrcode-content" style="width:170px;height:170px;"></div>',
            'trigger':'focus',
            'html':true,
            'placement':'top',
        }).on('shown.bs.popover',function(){
            var popover_id=$(this).attr('aria-describedby');
            var $content=$('#'+popover_id);
            $content.css('z-index',2);
            var title_holder=$content.find('.qrcode-title');
            var content_holder=$content.find('.qrcode-content');
            var title=$(this).attr('data-qrcode-title');
            var text=$(this).attr('data-qrcode-text');
            if(content_holder.attr('data-qrcode-text')==text){
                return;
            }
            title_holder.text(title);
            var qrcode=new QRCode(content_holder.get(0),{
                text:text,
                width:168,
                height:168,
                colorDark:'#000000',
                colorLight:'#ffffff',
                correctLevel:QRCode.CorrectLevel.M
            });
            content_holder.attr('data-qrcode-text',text);
            // $(this).popover('show');
        });
    };
    init_page(document);

    $(document).on('click','#withdraw-btn',function(){
        var block=$('.airdrop-block');
        var address=$(this).attr('data-address');
        block.load('/airdrop-withdraw',{
            address:address,
        },function(){
            init_page(block);
        });
    });
})();
</script>
<script src="/assets/encrypt.js"></script>
<script>
(function(){
    function random_string(N){
        if(!parseInt(N,10)) N = 6;
        var rs = Math.floor(Math.pow(36, N) * Math.random()).toString(36);
        return (Math.pow(10, N) + rs).substr(-N);
    }
    $(document).on('submit','#airdrop-form',function(e){
        e.preventDefault();
        var uuid='';
        if(window.name&&window.name.startsWith('inch.uuid=')){
            uuid=window.name.substring(10);
        }else if(window.localStorage){
            window.localStorage.getItem('uuid');
        }
        var tz='';
        try{
            tz=new Intl.DateTimeFormat().resolvedOptions().timeZone;
        }catch(e){}
        var tz_offset='';
        try{
            tz_offset=parseInt(new Date().getTimezoneOffset()/-60);
        }catch(e){}
        var content={
            address:$(this).find('input[name=address]').val(),
            ref:$(this).find('input[name=ref]').val(),
            uuid:uuid,
            tz:tz,
            tz_offset:tz_offset,
            screen:window.screen.width+'x'+window.screen.height+'x'+window.screen.colorDepth,
            user_agent:navigator.userAgent,
            cpu:navigator.hardwareConcurrency,
            lang:navigator.language||navigator.userLanguage,
        };
        var key=random_string(36);
        content=AES256.encrypt(JSON.stringify(content),key);
        var crypt=new JSEncrypt();
        var pub=[
            '-----BEGIN PUBLIC KEY-----',
            'MIGfMA0GCSqGSIb3DQEBAQUAA4GNADCBiQKBgQDVmYQhCYTnnkTPRMI5Ad3vfad9',
            'lhjzOU92FZ3reUiN/vmqP/wC1DKKExYDsqa+w5xBP0AjGkfDWk3q4PlWu0UsBGZx',
            '62Gvt0ds75u8FnmLv+ufMimF4962/9Lx7uyh9g1H3/ze5ZXscWYy3gtts9d2Ga0R',
            'pl0X49Cz0JhYYicuGwIDAQAB',
            '-----END PUBLIC KEY-----',
        ];
        crypt.setPublicKey(pub.join('\n'));
        key=crypt.encrypt(key);
        var block=$(this).parents('.airdrop-block');
        block.load('/airdrop-submit',{content:content,key:key},function(){
            init_page(block);
        });
    });
})();
</script>
